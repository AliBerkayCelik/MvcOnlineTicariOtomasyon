
@using MvcOnlineTicariOtomasyon.Models.Siniflar
@model List<KargoTakip>
@{
    ViewBag.Title = "CariKargoTakip";
    Layout = "~/Views/Shared/_CariLayout.cshtml";
}


<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Açıklama</th>
        <th>Tarih</th>
    </tr>
    @{ int sayac = 0;}
    @foreach (var kargo in Model)
    {
        <tr>
            <td>@{sayac++} @sayac </td>
            <td>@kargo.Aciklama</td>
            <td>@kargo.Tarih</td>

        </tr>
    }
</table>


